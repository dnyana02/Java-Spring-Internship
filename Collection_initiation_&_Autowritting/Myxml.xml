<?xml version="1.0" encoding="UTF-8"?>

<!-- spring framework file -->
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">




 
<beans>
   <bean id="triangle" class="org.demo.practice.DrawTriangle" autowire="byName">
   <property name="points">
     <list>
       <ref bean="point0"/>
       <ref bean="point1"/>
       <ref bean="point2"/>
     </list>
  </property>
  </bean>
  
  
    <bean id="point0" class="org.demo.practice.Point">
         
         <property name="x" value="0" />
         <property name="y" value="0" />
    
    </bean>
    
    <bean id="point1" class="org.demo.practice.Point">
         
         <property name="x" value="-20" />
         <property name="y" value="0" />
    
    </bean>
    
    <bean id="point2" class="org.demo.practice.Point">
         
         <property name="x" value="-20" />
         <property name="y" value="20" />
    
    </bean>
  
  <bean id="pointA" class="org.demo.practice.Point">
         
         <property name="x" value="0" />
         <property name="y" value="0" />
    
    </bean>
    
    <bean id="pointB" class="org.demo.practice.Point">
         
         <property name="x" value="-20" />
         <property name="y" value="0" />
    
    </bean>
    
    <bean id="pointC" class="org.demo.practice.Point">
         
         <property name="x" value="-20" />
         <property name="y" value="20" />
    
    </bean> 
  
  
   
  
</beans>       


